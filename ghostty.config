theme = Solarized Dark Higher Contrast
cursor-style = bar
command = /bin/zsh
window-theme = dark
window-colorspace = display-p3
clipboard-read = allow
click-repeat-interval = 500
bold-is-bright = true
auto-update-channel = stable
scrollback-limit = 268435456

font-family = MonoLisa
font-size = 20

font-feature = liga on
font-feature = zero on
font-feature = ss02 on
font-feature = ss15 on
font-feature = ss16 on
font-feature = ss13 on
font-feature = ss14 on
font-feature = ss11 on
